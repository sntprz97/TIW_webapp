<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>frontend_webapp</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <resource-ref>
	<description>My connection factory</description>
	<res-ref-name>tiwconnectionfactory</res-ref-name>
	<res-type>javax.jms.QueueConnectionFactory</res-type>
	<res-auth>Container</res-auth>
  </resource-ref>
  <resource-ref>
	<description>messages queue</description>
	<res-ref-name>tiwqueue</res-ref-name>
	<res-type>javax.jms.Queue</res-type>
	<res-auth>Container</res-auth>
  </resource-ref>
  <resource-ref>
	<description>messages topic</description>
	<res-ref-name>tiwtopic</res-ref-name>
	<res-type>javax.jms.Topic</res-type>
	<res-auth>Container</res-auth>
  </resource-ref>
  <servlet>
    <description></description>
    <display-name>ControlerServlet</display-name>
    <servlet-name>ControlerServlet</servlet-name>
    <servlet-class>ControlerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ControlerServlet</servlet-name>
    <url-pattern>*.html</url-pattern>
  </servlet-mapping>
</web-app>